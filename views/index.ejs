<script>
    try {
        if (getCookie("tipo") != "ADNIN" && getCookie("tipo") != "INVITADO") {

            var re = /^https?:\/\/[^/]+/i;
            window.location.href = re.exec(window.location.href)[0] + "/login";
            // window.location.href =window.location.href +"login";
        }
        else {
            if (getCookie("tipo") = "ADMIN") {
                window.location.href = "/principal_admin";
            }
            else {
                window.location.href = "/principal_user";
            }

        }
    }
    catch (err) {
        var re = /^https?:\/\/[^/]+/i;
        window.location.href = re.exec(window.location.href)[0] + "/login";
    }
</script>